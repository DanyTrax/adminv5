# HTACCESS MÍNIMO PARA ADMINV5

RewriteEngine On

# Proteger archivos críticos
<Files "config.php">
    Deny from all
</Files>

<Files "*.modelo.php">
    Deny from all
</Files>

<Files "error_log">
    Deny from all
</Files>

# Configuración PHP mínima
php_flag display_errors Off

# Sin listado de directorios
Options -Indexes